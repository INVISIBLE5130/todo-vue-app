AA,OAAA,EAAA,MAAA;AACA,KApBA;AAqBA,IAAA,IArBA,kBAqBA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,EAAA;AACA,YAAA;AACA,eAAA,KAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CAAA;AACA,SAFA,CAEA,OAAA,CAAA,EAAA;AACA,UAAA,YAAA,CAAA,UAAA,CAAA,OAAA;AACA;AACA;AACA;AA7BA,GAfA;AA8CA,EAAA,UAAA,EAAA;AAAA,IAAA,IAAA,EAAA,IAAA;AAAA,IAAA,UAAA,EAAA;AAAA;AA9CA,CAAA","sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <div class=\"row\">\r\n            <div class=\"col-12 py-5\">\r\n                <h1>{{ listName }}</h1>\r\n            </div>\r\n        </div>\r\n        <div class=\"row mb-3\">\r\n            <create-todo @on-new-todo=\"addTodo($event)\" />\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"col-12 col-sm-10 col-lg-6\">\r\n                <ul class=\"list-group\">\r\n                    <todo\r\n                            v-for=\"(todo, index) in todos\"\r\n                            :key=\"index\"\r\n                            :description=\"todo.description\"\r\n                            :completed=\"todo.completed\"\r\n                            @on-toggle=\"toggleTodo(todo)\"\r\n                            @on-delete=\"deleteTodo(todo)\"\r\n                            @on-edit=\"editTodo(todo, $event)\"\r\n                    />\r\n                </ul>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import Todo from \"./Todo.vue\";\r\n    import CreateTodo from \"./CreateTodo.vue\";\r\n\r\n    export default {\r\n        props: {\r\n            listName: String,\r\n        },\r\n        data() {\r\n            return {\r\n                todos: [\r\n                    { description: \"Learn Vue.js\", completed: false },\r\n                    { description: \"Create todo on Vue\", completed: false }\r\n                ],\r\n            };\r\n        },\r\n        mounted() {\r\n            this.init()\r\n        },\r\n        methods: {\r\n            addTodo(newTodo) {\r\n                this.todos.push({ description: newTodo, completed: false });\r\n                this.saveTodos()\r\n            },\r\n            toggleTodo(todo) {\r\n                todo.completed = !todo.completed;\r\n                this.saveTodos()\r\n            },\r\n            deleteTodo(deletedTodo) {\r\n                this.todos = this.todos.filter(todo => todo !== deletedTodo);\r\n                this.saveTodos()\r\n            },\r\n            editTodo(todo, newTodoDescription) {\r\n                todo.description = newTodoDescription;\r\n                this.saveTodos()\r\n            },\r\n            saveTodos() {\r\n                const parsed = JSON.stringify(this.todos);\r\n                localStorage.setItem('todos', parsed);\r\n            },\r\n            init() {\r\n                if (localStorage.getItem('todos')) {\r\n                    try {\r\n                        this.todos = JSON.parse(localStorage.getItem('todos'));\r\n                    } catch(e) {\r\n                        localStorage.removeItem('todos');\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        components: { Todo, CreateTodo },\r\n    };\r\n</script>"],"sourceRoot":"src/components"}]}